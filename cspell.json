{
	"version": "0.1",
	"language": "en",
	"dictionaries": [
		"csharp",
		"dictionary"
	],
    "caseSensitive": true,
	"dictionaryDefinitions": [
		{ "name": "dictionary", "path": "./cspell-dictionary.txt" }
	],
    "flagWords": [
        "hte"
    ],
	"ignoreRegExpList": [
		"\\(.+\\/.+\\..+\\)",
		"\\]: .+\\/.+\\..+$",
		"[a-zA-Z]{13}-[\\w-]{42}",
		"\\d{3,}:[\\w-_]{36}",
		"\"[\\w-_]{20,}\"",
		"@[\\w-_]+",
		"^```(?:json)$[\\s\\S]+?^```$"
	]
}
